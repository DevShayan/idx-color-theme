			],
			"settings": {
				"foreground": "#0075A2"
			}
		},
		{
			"scope": "support",
			"settings": {
				"foreground": "#264DCB"
			}
		},
		{
			"scope": [
				"support.type.property-name",
				"entity.name.tag.yaml"
			],
			"settings": {
				"foreground": "#0075A2"
			}
		},
		{
			"scope": "meta.property-name",
			"settings": {
				"foreground": "#264DCB"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#D07826"
			}
		},
		{
			"scope": "variable.other",
			"settings": {
				"foreground": "#1F2939"
			}
		},
