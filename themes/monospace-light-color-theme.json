				"fontStyle": "italic"
			}
		},
		{
			"scope": "carriage-return",
			"settings": {
				"foreground": "#FAFBFE",
				"background": "#C43058",
				"fontStyle": "italic underline"
			}
		},
		{
			"scope": "message.error",
			"settings": {
				"foreground": "#AD1C48"
			}
		},
		{
			"scope": "string source",
			"settings": {
				"foreground": "#1F2939"
			}
		},
		{
